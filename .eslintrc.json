{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": 1,
        "linebreak-style": 0,
        "object-curly-spacing": 0,
        "no-extra-semi": 1,
        "indent": 1,
        "no-var": 2,
        "comma-dangle": 0,
        "spaced-comment": 1,
        "require-jsdoc": 0,
        "camelcase": 0,
        "operator-linebreak": 0,
        "no-unused-vars": 1,
        "no-trailing-spaces": 1,
        "max-len": 0,
        "arrow-parens": 0,
        "react/jsx-filename-extension": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "react/jsx-indent-props": 1,
        "object-curly-newline": 0,
        "react/prop-types": 0,
        "react/jsx-props-no-spreading": 0
    }
}
